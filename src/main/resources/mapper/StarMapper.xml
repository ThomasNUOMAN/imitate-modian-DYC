<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhiyou100.imitatemodian.mapper.StarMapper">


    <delete id="deleteByTwoId" parameterType="Star">
         DELETE
        FROM
            star
        WHERE
            star_user_id = #{userId}
            AND star_project_base_id = #{projectId}

    </delete>
</mapper>